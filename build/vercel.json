{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@now/node"
        }
    ],
    "routes": [
        {
            "src": "/swagger-ui.css",
            "dest": "/swagger-ui.css",
            "headers": {
                "x-my-header": "x-my-header"
            }
        },
        {
            "src": "/(.*)",
            "dest": "server.js"
        }
    ]
}
